{
  "expo": {
    "name": "WeatherApp",
    "slug": "WeatherApp",
    "scheme": "weatherapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": "1.0.0",
      "foregroundImage": "./src/assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "runtimeVersion": "1.0.0"
      },
      "package": "com.bhardwajaditya.WeatherApp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "runtimeVersion": "1.0.0",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "196a86b5-737c-46dc-9c4e-4c0d021ee506"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/196a86b5-737c-46dc-9c4e-4c0d021ee506"
    }
  }
}
